{
  "compilerOptions": {
    "alwaysStrict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": ["es6", "dom", "es2017", "esnext"],
    "module": "esnext",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "esnext",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "rootDir": ".",
    "baseUrl": ".",
    "noErrorTruncation": true,
    "experimentalDecorators": true,
    "pretty": true,
    "incremental": true,
    "plugins": [
      {
        "name": "next",
      }
    ],
    "paths": {
      "@app/*": ["./src/app/*"],
      "@emails/*": ["./src/emails/*"],
      "@components/*": ["./src/app/components/*"],
      "@ui/*": ["src/app/utils/ui/*"],
      "@modules/*": ["./src/app/modules/*"],
      "@constants/*": ["./src/app/constants/*"],
      "@icons/*": ["./src/app/utils/icons/*"],
      "@types/*": ["./src/app/types/*"],
      "@atoms/*": ["./src/app/components/atoms"],
      "@molecules/*": ["./src/app/components/molecules"],
      "@organisms/*": ["./src/app/components/organisms"],
      "@templates/*": ["./src/app/components/templates"],
      "@hooks/*": ["./src/app/hooks/*"],
      "@helpers/*": ["./src/app/helpers/*"],
      "@utils/*": ["./src/app/utils/*"],
      "@hoc/*": ["./src/app/hoc/*"],
      
    }
  },
  "exclude": ["node_modules", "**/*.spec.ts", "dist", ".next", "out", "next.config.js"],
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
}